{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Propsis",
  "description": "Deal pipeline and analysis platform for real estate investors. Track deals, run financial analysis, and close faster.",
  "colors": {
    "primary": "#FFFFFF",
    "light": "#E5E7EB",
    "dark": "#111111"
  },
  "appearance": {
    "default": "dark"
  },
  "logo": {
    "light": "/images/logo-light.svg",
    "dark": "/images/logo-dark.svg",
    "href": "https://propsis.io"
  },
  "favicon": "/images/favicon.png",
  "fonts": {
    "heading": {
      "family": "Inter"
    },
    "body": {
      "family": "Inter"
    }
  },
  "icons": {
    "library": "lucide"
  },
  "styling": {
    "eyebrows": "breadcrumbs",
    "codeblocks": "dark"
  },
  "navbar": {
    "links": [
      {
        "label": "App",
        "href": "https://app.propsis.io"
      },
      {
        "type": "github",
        "href": "https://github.com/launchtheoryai/propsis"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.propsis.io/signup"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "User Guide",
        "icon": "book-open",
        "groups": [
          {
            "group": "Getting Started",
            "icon": "rocket",
            "pages": [
              "user-guide/getting-started/introduction",
              "user-guide/getting-started/quickstart",
              "user-guide/getting-started/key-concepts"
            ]
          },
          {
            "group": "Deal Pipeline",
            "icon": "kanban",
            "pages": [
              "user-guide/deal-pipeline/overview",
              "user-guide/deal-pipeline/pipeline-stages",
              "user-guide/deal-pipeline/deal-cards",
              "user-guide/deal-pipeline/deal-detail",
              "user-guide/deal-pipeline/search-and-filter",
              "user-guide/deal-pipeline/activity-tracking"
            ]
          },
          {
            "group": "Financial Calculators",
            "icon": "calculator",
            "pages": [
              "user-guide/calculators/overview",
              "user-guide/calculators/fix-and-flip",
              "user-guide/calculators/brrrr",
              "user-guide/calculators/rental-analysis",
              "user-guide/calculators/rehab-estimator"
            ]
          },
          {
            "group": "CMA AI Tool",
            "icon": "brain",
            "pages": [
              "user-guide/cma-tool/overview",
              "user-guide/cma-tool/comparable-search",
              "user-guide/cma-tool/arv-estimates",
              "user-guide/cma-tool/market-trends"
            ]
          },
          {
            "group": "Property Search",
            "icon": "search",
            "pages": [
              "user-guide/property-search/overview",
              "user-guide/property-search/property-lookup",
              "user-guide/property-search/saved-searches"
            ]
          },
          {
            "group": "Document Management",
            "icon": "folder",
            "pages": [
              "user-guide/documents/overview",
              "user-guide/documents/upload-and-organize",
              "user-guide/documents/compliance-tracking"
            ]
          }
        ]
      },
      {
        "tab": "Developer Docs",
        "icon": "code",
        "groups": [
          {
            "group": "Architecture",
            "icon": "blocks",
            "pages": [
              "developer/architecture/overview",
              "developer/architecture/tech-stack",
              "developer/architecture/project-structure",
              "developer/architecture/design-system"
            ]
          },
          {
            "group": "API Reference",
            "icon": "square-terminal",
            "pages": [
              "developer/api-reference/overview",
              "developer/api-reference/deals",
              "developer/api-reference/dashboard",
              "developer/api-reference/attom-integration"
            ]
          },
          {
            "group": "Frontend",
            "icon": "palette",
            "pages": [
              "developer/frontend/getting-started",
              "developer/frontend/components",
              "developer/frontend/hooks-and-state",
              "developer/frontend/dashboard-widgets"
            ]
          },
          {
            "group": "Backend",
            "icon": "server",
            "pages": [
              "developer/backend/getting-started",
              "developer/backend/database-schema",
              "developer/backend/authentication",
              "developer/backend/background-jobs"
            ]
          }
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/launchtheoryai/propsis",
      "linkedin": "https://www.linkedin.com/company/launchtheoryai",
      "x": "https://x.com/launchtheoryai"
    }
  },
  "search": {
    "prompt": "Search Propsis docs..."
  },
  "contextual": {
    "options": ["copy", "view", "claude"]
  }
}
